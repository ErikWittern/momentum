<div class="text-center">
  <img src="/logo_large.png" class="img-responsive center-block" />
  <br/>
  <%- if devise_mapping.omniauthable? %>
    <%- resource_class.omniauth_providers.each do |provider| %>
      <%= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), class: 'btn btn-info' %><br />
    <% end -%>
  <% end -%>
</div>
